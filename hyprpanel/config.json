{
  "bar.layouts": {
    "*": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "systray",
        "clock",
        "notifications"
      ]
    }
  },
  "bar.customModules.storage.paths": [
    "/"
  ],
  "theme.font.size": "1.1rem",
  "theme.bar.transparent": false,
  "theme.bar.opacity": 1,
  "theme.bar.menus.background": "rgba(17, 17, 27, 0.55)",
  "theme.bar.menus.cards": "rgba(30, 30, 46, 0.55)",
  "theme.bar.menus.popover.background": "#ed333b",
  "theme.bar.menus.menu.notifications.background": "#00000001",
  "theme.bar.menus.menu.dashboard.background.color": "#00000001",
  "theme.bar.menus.menu.dashboard.card.color": "#241f3183",
  "menus.clock.time.military": true,
  "menus.clock.time.hideSeconds": true,
  "bar.workspaces.show_numbered": true,
  "bar.clock.showTime": true,
  "theme.bar.buttons.clock.enableBorder": false,
  "bar.clock.format": "%a %b %d  %I:%M %p",
  "bar.battery.label": false,
  "bar.battery.enabled": true,
  "bar.battery.showIcon": false,
  "bar.bluetooth.label": true,
  "menus.clock.weather.enabled": true,
  "menus.clock.weather.unit": "metric",
  "theme.bar.menus.menu.clock.background.color": "#00000001",
  "theme.bar.menus.menu.clock.card.color": "#ff780033",
  "scalingPriority": "hyprland",
  "theme.bar.border.location": "none",
  "bar.launcher.autoDetectIcon": true,
  "theme.osd.enable": true
}